<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/dashboard_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:screenOrientation="landscape">

    <com.github.anastr.speedviewlib.ImageSpeedometer
        android:id="@+id/speedometer"
        android:layout_width="478dp"
        android:layout_height="529dp"
        android:layout_x="101dp"
        android:layout_y="77dp"
        app:sv_image="@drawable/examplespeedometer"
        app:sv_indicator="NormalSmallIndicator"
        app:sv_speedTextColor="@android:color/transparent"
        app:sv_speedometerWidth="35dp"
        app:sv_unitTextColor="@android:color/transparent" />

    <com.github.anastr.speedviewlib.ImageSpeedometer
        android:id="@+id/tachometer"
        android:layout_width="489dp"
        android:layout_height="483dp"
        android:layout_x="706dp"
        android:layout_y="73dp"
        app:sv_centerCircleColor="#b2f941ff"
        app:sv_highSpeedColor="#ec2f33"
        app:sv_image="@drawable/exampletachometer"
        app:sv_indicator="NormalSmallIndicator"
        app:sv_indicatorColor="#2b38e6"
        app:sv_lowSpeedColor="#58ed21"
        app:sv_markColor="#99000000"
        app:sv_mediumSpeedColor="#edd029"
        app:sv_speedTextColor="@android:color/transparent"
        app:sv_speedometerWidth="35dp"
        app:sv_unitTextColor="@android:color/transparent" />

    <com.github.anastr.speedviewlib.ImageSpeedometer
        android:id="@+id/oilpressuregauge"
        android:layout_width="218dp"
        android:layout_height="218dp"
        android:layout_x="178dp"
        android:layout_y="570dp"
        app:sv_image="@drawable/exampleoiltemp"
        app:sv_speedTextColor="@android:color/transparent"
        app:sv_unitTextColor="@android:color/transparent" />

    <com.github.anastr.speedviewlib.ImageSpeedometer
        android:id="@+id/temperaturegauge"
        android:layout_width="220dp"
        android:layout_height="190dp"
        android:layout_x="868dp"
        android:layout_y="565dp"
        app:sv_image="@drawable/examplewatertemp"
        app:sv_speedTextColor="@android:color/transparent"
        app:sv_unitTextColor="@android:color/transparent"/>

    <TextView
        android:layout_width="51dp"
        android:layout_height="wrap_content"
        android:layout_x="695dp"
        android:layout_y="692dp"
        android:text="OBD"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/obd_status"
        android:layout_width="37dp"
        android:layout_height="wrap_content"
        android:layout_x="708dp"
        android:layout_y="739dp"
        android:text=""
        android:textSize="18sp" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="794dp"
        android:layout_y="691dp"
        android:text="BT"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/bt_status"
        android:layout_width="37dp"
        android:layout_height="wrap_content"
        android:layout_x="799dp"
        android:layout_y="734dp"
        android:text=""
        android:textSize="18sp" />


    <SeekBar
        android:id="@+id/seekBar"
        android:layout_width="318dp"
        android:layout_height="73dp"
        android:layout_x="523dp"
        android:layout_y="564dp"
        android:max="100" />


    <Button
        android:id="@+id/ok"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="411dp"
        android:layout_y="593dp"
        android:text="ok" />

    <CheckBox
        android:id="@+id/withTremble"
        android:layout_width="206dp"
        android:layout_height="56dp"
        android:layout_x="437dp"
        android:layout_y="657dp"
        android:checked="true"
        android:text="withTremble" />

    <TextView
        android:id="@+id/textSpeed"
        android:layout_width="133dp"
        android:layout_height="wrap_content"
        android:layout_x="19dp"
        android:layout_y="605dp"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textSize="50pt" />


</AbsoluteLayout>